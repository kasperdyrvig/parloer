---
layout: standard
---
<div>
{{content | safe}}
</div>

<h2 class="mt-lg mb-sm">Lektioner</h2>
<div class="list-group nav-list-group lessonList">
    {%- for lesson in collections[getLessonsOf] | sort(false, false, 'data.lessonNo') %}
    <a class="list-group-item" href="{{lesson.url | url}}" data-lessonnumber="{{lesson.data.lessonNo}}">{{lesson.data.metaTitle}} <span>&#9745;</span></a>
    {%- endfor %}
</div>

<p><a class="btn btn-small btn-white btn-inline mb-lg" href="parloer">Støttesætninger</a></p>

<details class="mb-lg">
    <summary>Til læreren</summary>
    {% for teachernote in collections.teachernotes %}
        {% if teachernote.data.partOfModule == moduleNo %}
            {{teachernote.content | safe}}
        {% endif %}
    {% endfor %}
</details>